$cd Documents/rob-prog/

$sudo apt-get install python-pip

$pip install --upgrade pip

$sudo pip install setuptools

$sudo pip install mbed-cli

$sudo mbed import mbed-os-example-blinky //necessite git

$cd mbed-os-example-blinky/

$sudo chmod 777 main.cpp 

$sudo mbed compile -t GCC_ARM -m NUCLEO_F303K8

